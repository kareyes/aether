{
  "name": "@kareyes/aether",
  "version": "0.0.11",
  "description": "A Svelte 5 UI component library built with Tailwind CSS",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kareyes/aether.git"
  },
  "homepage": "https://github.com/kareyes/aether",
  "bugs": {
    "url": "https://github.com/kareyes/aether/issues"
  },
  "keywords": [
    "svelte",
    "svelte5",
    "components",
    "ui",
    "tailwindcss",
    "component-library"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:package": "svelte-kit sync && svelte-package && node scripts/clean-dist.js",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepublishOnly": "pnpm run build:package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.stories.*"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    },
    "./icons": {
      "types": "./dist/icons.d.ts",
      "svelte": "./dist/icons.js",
      "default": "./dist/icons.js"
    },
    "./styles": "./dist/styles/theme.css",
    "./styles/theme.css": "./dist/styles/theme.css"
  },
  "peerDependencies": {
    "@lucide/svelte": "^0.561.0",
    "svelte": "^5.0.0",
    "tailwindcss": "^4.0.0"
  },
  "dependencies": {
    "@internationalized/date": "^3.10.0",
    "@tanstack/table-core": "^8.21.3",
    "bits-ui": "^2.15.4",
    "clsx": "^2.1.1",
    "mode-watcher": "^1.1.0",
    "svelte-sonner": "^1.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.2.2",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@chromatic-com/storybook": "^4.1.1",
    "@lucide/svelte": "^0.561.0",
    "@playwright/test": "^1.55.1",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/sveltekit": "^9.1.10",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/kit": "^2.43.2",
    "@sveltejs/package": "^2.3.10",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^22",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "formsnap": "^2.0.1",
    "playwright": "^1.56.0",
    "storybook": "^9.1.10",
    "svelte": "^5.39.5",
    "svelte-check": "^4.3.2",
    "sveltekit-superforms": "^2.28.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@tailwindcss/oxide"
    ]
  }
}